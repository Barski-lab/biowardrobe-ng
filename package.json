{
  "name": "biowardrobe-ng",
  "private": true,
  "postinstall": "typings i",
  "scripts": {
    "meteor": "ROOT_URL= MONGO1_URL=mongodb://localhost:27017/biowardrobe-ng meteor run --port 3030 --settings ./private/settings.json",
    "debug": "ROOT_URL= MONGO_URL=mongodb://localhost:27017/biowardrobe-ng meteor --port 3030 --settings ./private/settings.json --raw-logs |./node_modules/bunyan/bin/bunyan",
    "build": "AOT=1 ROLLUP=1 meteor build ../output && scp ../output/biowardrobe-ng.tar.gz devel:/meteor/biowardrobe-ng/ && rm -rf ../output",
    "test-full": "TEST_BROWSER_DRIVER=chrome meteor test --once --full-app --driver-package meteortesting:mocha --settings private/settings.json",
    "test": "ROOT_URL= meteor test --once --driver-package meteortesting:mocha --settings private/settings.json --raw-logs |./node_modules/bunyan/bin/bunyan",
    "params": "AOT=0 ROLLUP=0 TOOL_NODE_FLAGS=\"--max_old_space_size=4096\" "
  },
  "devDependencies": {
    "@types/es6-shim": "^0.31.35",
    "@types/meteor": "^1.4.13",
    "@types/meteor-roles": "^1.2.29",
    "@types/mocha": "^2.2.46",
    "@types/node": "^8.5.8",
    "@types/underscore": "^1.8.6",
    "bunyan": "^2.0.2",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/cdk": "2.0.0-beta.12",
